url={https://www.proquest.com/scholarly-journals/design-test-spatial-cognitive-training-evaluation/docview/2828006191/se-2},
}

@article{
author={Vidak,Andrej and Šapić,Iva M. and Mešić,Vanes},
year={2023/01//},
month={Jan 2023},
title={Verifying the equation for centripetal force: an augmented reality approach},
journal={Physics Education},
volume={58},
