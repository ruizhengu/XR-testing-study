@article{
author={Manfredini,F. and Malagoni,A. M. and Litmanen,H. and Zhukovskaja,L. and Jeannier,P. and Dal Follo,D. and Felisatti,M. and Besseberg,A. and Geistlinger,M. and Bayer,P. and Carrabre,J. E.},
year={2011/03//},
