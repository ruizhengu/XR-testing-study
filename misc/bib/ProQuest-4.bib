@article{
author={Tsili,Athina C. and Astrakas,Loukas G. and Ntorkou,Alexandra and Giannakis,Dimitrios and Stavrou,Sotirios and Maliakas,Vasilios and Sofikitis,Nikolaos and Argyropoulou,Maria I.},
