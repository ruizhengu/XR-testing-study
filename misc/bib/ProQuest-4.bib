@article{
author={Barnden,Leighton and Kwiatek,Richard and Lau,Yiu and Hutton,Brian and Thurfjell,Lennart and Pile,Kevin and Rowe,Christopher},
year={2000/02//},
month={Feb 2000},
title={Validation of fully automatic brain SPET to MR co-registration},
journal={European journal of nuclear medicine},
volume={27},
number={2},
pages={147-154},
note={Copyright - Springer-Verlag Berlin Heidelberg 2000; Last updated - 2023-11-23},
abstract={Fully automatic co-registration of functional to anatomical brain images using information intrinsic to the scans has been validated in a clinical setting for positron emission tomography (PET), but not for single-photon emission tomography (SPET). In this paper we evaluate technetium-99m hexamethylpropylene amine oxime to magnetic resonance (MR) co-registration for five fully automatic methods. We attached six small fiducial markers, visible in both SPET and MR, to the skin of 13 subjects. No increase in the radius of SPET acquisition was necessary. Distortion of the fiducial marker distribution observed in the SPET and MR studies was characterised by a measure independent of registration and three subjects were excluded on the basis of excessive distortion. The location of each fiducial marker was determined in each modality to sub-pixel precision and the inter-modality distance was averaged over all markers to give a fiducial registration error (FRE). The component of FRE excluding the variability inherent in the validation method was estimated by computing the error transformation between the arrays of MR marker locations and registered SPET marker locations. When applied to the fiducial marker locations this yielded the surface registration error (SRE), and when applied to a representative set of locations within the brain it yielded the intrinsic registration error (IRE). For the best method, mean IRE was 1.2 mm, SRE 1.5 mm and FRE 2.4 mm (with corresponding maxima of 3.3, 4.3 and 5.0 mm). All methods yielded a mean IRE <3 mm. The accuracy of the most accurate fully automatic SPET to MR co-registration was comparable with that published for PET to MR. With high standards of calibration and instrumentation, intra-subject cerebral SPET to MR registration accuracy of <2 mm is attainable.PUBLICATION ABSTRACT]},
keywords={Medical Sciences--Radiology And Nuclear Medicine; Single-photon emission computed tomography; Magnetic resonance imaging},
isbn={03406997},
language={English},
url={https://www.proquest.com/scholarly-journals/validation-fully-automatic-brain-spet-mr-co/docview/1026699091/se-2},
}

@article{
author={Allingham,Philip V.},
year={2000},
month={2000},
title={The Ontario secondary school literacy test: Mr Harris's high-stakes version of BC's foundation skills assessment},
journal={English Quarterly},
volume={32},
number={3},
pages={68-69},
note={Copyright - Copyright Canadian Council of Teachers of English Language Arts 2000; Last updated - 2024-04-19; SubjectsTermNotLitGenreText - Ontario Canada; Writing Tests; Resource Materials; Canada; Grade 10},
abstract={The Resource Guide concedes that "in most classroom writing assignments, students follow a process that includes access to dictionaries, peer and teacher feedback and opportunities to revise and edit over time" (5), but that this "ondemand" series of writing tasks did not give students access to these customary resources, although time for self-editing could have been incorporated. Sadly, the Ontario Ministry of Education is using this highly 'inauthentic' assessment of Grade 9/10 writing to question the competency of both students and teachers. Not surprisingly, the failure rates are higher in rural Northern Ontario; in the Lakehead Schools (S. D. 157, Thunder Bay), for example, 29% failed writing as opposed to 24% provincially. As is the case with B.C.'s FSA, results reflect regional, linguistic, and socio-economic disparities rather than differences in the quality of teaching. For example, 44% of the Grade Tens in B. C. School District #57 (Prince George, a northern city the size of Thunder Bay with a resource-based economy, a college, and a small university) failed the composition on the FSA. Whereas the B.C. Ministry does not itself provide such district data (one must contact the individual districts for such information), in Ontario one may access such information from the website of the Education Quality Assessment Office (EQAO), suggesting that the general public is being invited to compare the performance of various school districts. As recently re-elected British Columbia Teachers' Federation President, David Chudnovsky, remarked in "Ranking Schools No Way to Improve Learning," spending millions of dollars merely to label learners and impute poor results to professional incompetency is no way to enhance classroom learning. Many in Ontario are already speculating as to whether the Ministry intends to retain failing students in Grade Ten, or whether these literacy and numeracy tests are merely precursors of a full-blown school-leaving assessment program akin to B.C.'s provincial examinations.},
keywords={Literature; Educational Policy; Assessment; Ontario; Literacy; Secondary school; Writing; Teacher education; Students; Evaluation; Secondary level; Provincial examinations; Testing; Ontario Canada; Canada; Grade 10; Writing Tests; Resource Materials; 9172:Canada},
isbn={00138355},
language={English},
url={https://www.proquest.com/scholarly-journals/ontario-secondary-school-literacy-test-mr-harriss/docview/233296479/se-2},
}