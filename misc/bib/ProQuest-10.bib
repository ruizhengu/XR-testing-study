}

@article{
author={Mildner,T. and Norris,D. G. and Schwarzbauer,C. and Wiggins,C. J.},
year={2001/11//},
month={November 2001},
title={A qualitative test of the balloon model for BOLD-based MR signal changes at 3T},
journal={Magnetic resonance in medicine},
volume={46},
number={5},
pages={891-899},
note={Date completed - 2001-12-14; Date created - 2001-10-25; Date revised - 2024-01-30; Last updated - 2024-02-08},
abstract={The aim of this study was to adapt the balloon model for BOLD-based MR signal changes to a magnetic field strength of 3T and to examine its validity. The simultaneous measurement of BOLD and diffusion-weighted BOLD responses was performed. The amplitude of the BOLD peak was found to be similar for all subjects when a short visual stimulus of 6 sec was used. The rise-time to the BOLD peak and the shape and depth of the poststimulus undershoot varied significantly. A fit of the experimental BOLD responses was found to be possible by use of parameters within a reasonable physiological range. The relations between these parameters and their influence on the modeled BOLD responses is discussed. A prediction of the balloon model is the occurrence of a BOLD overshoot, i.e., a lag between the changes of the blood volume and the blood flow after the start of the stimulation. Experimental evidence for the existence of a BOLD overshoot is presented.},
keywords={Index Medicus; Image Processing, Computer-Assisted; Humans; Photic Stimulation; Brain -- physiology; Magnetic Resonance Imaging -- methods; Brain -- metabolism},
isbn={0740-3194, 0740-3194},
