@article{
author={Mansard,Catherine D. and Canet Soulas,Emmanuelle,P. and Anwander,Alfred and Chaabane,Linda and Neyran,Bruno and Serfaty,Jean-Michel and Magnin,Isabelle E. and Douek,Philippe C. and Orkisz,Maciej},
year={2004/02//},
