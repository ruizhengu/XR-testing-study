}

@article{
author={Omary,Reed A. and Henseler,Kevin P. and Unal,Orhan and Smith,Randall J. and Ryu,Robert K. and Resnick,Scott A. and Saker,Mark B. and Chrisman,Howard B. and Frayne,Richard and Finn,J. P. and Li,Debiao and Grist,Thomas M.},
