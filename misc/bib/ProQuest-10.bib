author={Meissner,Oliver A. and Rieger,Johannes and Rieber,Johannes and Klauss,Volker and Siebert,Uwe and Tató,Federico and Pfeifer,Klaus-Juergen and Reiser,Maximilian and Hoffmann,Ulrich},
year={2003/02//},
