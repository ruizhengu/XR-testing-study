url={https://www.proquest.com/scholarly-journals/financial-liberalisation-economic-growth-nigeria/docview/1531431126/se-2},
}

@article{
author={Owusu,Erasmus L. and Odhiambo,Nicholas M.},
year={2014/03//},
month={Mar 2014},
title={Stock market development and economic growth in Ghana: an AR - -bounds testing approach},
journal={Applied economics letters},
volume={21},
