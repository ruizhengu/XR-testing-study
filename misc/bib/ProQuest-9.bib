url={https://www.proquest.com/scholarly-journals/performance-blood-monitoring-sports-artificial/docview/850562027/se-2},
}

@article{
author={Aulanier,A-L and Doiron,A. L. and Shepherd,R. D. and Rinker,K. D. and Frayne,R. and Andersen,L. B.},
year={2011/02//},
